<main>
  <form action="/addState" method="POST">
    <h1 class="h4 mt-4 mb-4">Please select your state</h1>
    <div class="mb-3">
      <label for="formControlInputCountry" class="form-label">State</label>
      <select class="form-select" type="text" id="state" name="state">
        <option value="State1">State1</option>
        <option value="State2">State2</option>
      </select>
    </div>
    <button class="mt-4 w-100 btn btn-dark" type="submit">Next</button>
  </form>
</main>
